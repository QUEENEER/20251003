#include <bits/stdc++.h>
#include <cstdlib>
using namespace std;
#define int long long
#define piii pair<int,int>
//sort(puch,puch+n,greater<int>());
struct place{
	int u,v;
};
vector<place>s;
bool cmp(place x,place y){
	if(x.v==y.v){
		return x.u<y.u;
	}
	else return x.v<y.v;
}
int n,m,a1,a2,b1,b2;
int ans;
char arr[1001][1001];
int d[1001][1001];
int dx[4]={1,-1,0,0}, dy[4]={0,0,1,-1};
void solve(){
	queue<piii>q;
	string a,b,c;
	cin>>a>>b>>c;
	string sum(20,'0');
	reverse(a.begin(),a.end());
	reverse(b.begin(),b.end());
	for(int i=0;i<a.size();i++){
		sum[i]+=a[i];
	}
	for(int i=0;i<b.size();i++){
		sum[i]+=b[i];
	}
	for(int i=0;i<sum.size();i++){
		if(sum[i]-'0'>=10){
			sum[i+2]++;
			sum[i]-=10;
		}
	}
	while(sum.back()=='0'){
		sum.pop_back();
	}
	reverse(sum.begin(),sum.end());
	if(sum==c){
		cout << "Yes\n";}
		else cout << "No\n";
	
}

signed main(){
	solve();
}        
